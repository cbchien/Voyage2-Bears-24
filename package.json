{
	"name": "chingu-dashboard",
	"version": "1.0.1",
	"description": "Chingu Dashboard",
	"main": "./src/www.js",
	"scripts": {
		"serve": "node ./src/www.js",
		"start": "DEBUG=express:application,app:* concurrently --kill-others --raw 'npm run build:dev' 'npm run start:dev'",
		"start:dev": "nodemon -w ./src/server ./src/www.js",
		"build:dev": "webpack --env.in ./src/client/index.js --env.out bundle --watch --progress",
		"build:prod": "webpack --env.in ./src/client/index.js --env.out bundle --env.prod"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chingu-coders/Voyage2-Bears-24.git"
	},
	"engines": {
		"node": ">=8.6.0"
	},
	"keywords": [
		"chingu",
		"voyage2",
		"freecodecamp",
		"fcc"
	],
	"author": "Voyage2-Bears-24",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/chingu-coders/Voyage2-Bears-24/issues"
	},
	"homepage": "https://github.com/chingu-coders/Voyage2-Bears-24#readme",
	"dependencies": {
		"antd": "2.13.4",
		"express": "4.16.1",
		"google-auth-library": "0.11.0",
		"googleapis": "22.2.0",
		"passport": "0.4.0",
		"react": "16.0.0",
		"react-dom": "16.0.0"
	},
	"devDependencies": {
		"@types/express": "4.0.37",
		"@types/node": "8.0.31",
		"@types/react": "16.0.7",
		"@types/webpack": "3.0.12",
		"babel-eslint": "8.0.1",
		"cache-loader": "1.0.3",
		"clean-webpack-plugin": "0.1.17",
		"concurrently": "3.5.0",
		"eslint": "4.7.2",
		"eslint-config-airbnb": "15.1.0",
		"eslint-loader": "^1.9.0",
		"eslint-plugin-import": "2.7.0",
		"eslint-plugin-jsx-a11y": "5.1.1",
		"eslint-plugin-react": "7.4.0",
		"happypack": "^4.0.0",
		"nodemon": "1.12.1",
		"rimraf": "2.6.2",
		"source-map-loader": "0.2.2",
		"thread-loader": "1.1.1",
		"ts-loader": "2.3.7",
		"typescript": "2.5.3",
		"uglifyjs-webpack-plugin": "1.0.0-beta.2",
		"webpack": "3.6.0"
	}
}

{
  "name": "chingu-dashboard",
  "version": "1.0.1",
  "description": "Chingu Dashboard",
  "main": "./src/www.js",
  "scripts": {
    "serve": "node ./src/www.js",
    "start": "DEBUG=api:*,test:*,service:* concurrently --kill-others --raw 'npm run build:dev' 'npm run start:dev'",
    "start:new": "concurrently --kill-others --raw 'npm run build:dev' 'npm run start:dev'",
    "start:dev": "nodemon -w ./src/server ./src/www.js",
    "start:window": "set DEBUG=api:*,test:* & concurrently --kill-others --raw \"npm run build:dev\" \"npm run start:dev\"",
    "build:dev": "webpack --env.in ./src/client/index.js --env.out bundle --watch --display minimal",
    "build:prod": "webpack --env.in ./src/client/index.js --env.out bundle --env.prod --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chingu-coders/Voyage2-Bears-24.git"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "keywords": [
    "chingu",
    "voyage2",
    "freecodecamp",
    "fcc"
  ],
  "author": "Voyage2-Bears-24",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chingu-coders/Voyage2-Bears-24/issues"
  },
  "homepage": "https://github.com/chingu-coders/Voyage2-Bears-24#readme",
  "dependencies": {
    "@types/express": "4.0.37",
    "@types/express-session": "1.15.3",
    "@types/node": "8.0.32",
    "@types/prop-types": "15.5.2",
    "@types/react-router-dom": "4.0.8",
    "@types/redux": "3.6.0",
    "@types/socket.io": "1.4.31",
    "@types/socket.io-client": "1.4.30",
    "antd": "2.13.4",
    "debug": "^3.1.0",
    "express": "4.16.1",
    "express-session": "1.15.6",
    "google-auth-library": "0.11.0",
    "googleapis": "22.2.0",
    "history": "4.7.2",
    "invariant": "2.2.2",
    "jquery": "^3.2.1",
    "passport": "0.4.0",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2",
    "socket.io": "2.0.3",
    "socket.io-client": "2.0.3",
    "tslib": "1.7.1"
  },
  "devDependencies": {
    "babel-eslint": "8.0.1",
    "cache-loader": "1.0.3",
    "clean-webpack-plugin": "0.1.17",
    "concurrently": "3.5.0",
    "eslint": "4.7.2",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "happypack": "^4.0.0",
    "nodemon": "1.12.1",
    "null-loader": "0.1.1",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.2",
    "thread-loader": "1.1.1",
    "ts-loader": "2.3.7",
    "typescript": "2.5.3",
    "uglifyjs-webpack-plugin": "1.0.0-beta.2",
    "webpack": "3.6.0"
  }
}
